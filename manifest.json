{
  "manifest_version": 3,
  "name": "EQ Extension",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabCapture", "offscreen"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icon.png"
  }
}
